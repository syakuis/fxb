{
  "name": "fxb-security-react",
  "version": "1.0.0",
  "description": "Spring security UI with React",
  "author": "Syaku (최석균) <http://syaku.tistory.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/syakuis/fxb"
  },
  "keywords": [
    "javascript",
    "es",
    "react"
  ],
  "main": "dist/fxb-security-react.js",
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/__tests__/**/*.js?(x)"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleNameMapper": {
      "^_resources$": "<rootDir>/src/resources/",
      "^_commons$": "<rootDir>/src/commons/",
      "^_config$": "<rootDir>/src/config/",
      "^_components$": "<rootDir>/src/components/",
      "^_apps$": "<rootDir>/src/apps/",
      "^_layouts$": "<rootDir>/src/layouts/",
      "^_reducers$": "<rootDir>/src/reducers/",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "config": {
    "filename": "fxb-security-react",
    "publicPath": "/",
    "output": "dist",
    "src": "src",
    "entry": "./src/index.js",
    "port": 8088,
    "vendors": [
      "babel-polyfill",
      "react",
      "react-dom",
      "react-router-dom",
      "redux",
      "react-redux",
      "store",
      "axios",
      "nprogress"
    ],
    "externals": {}
  },
  "scripts": {
    "jest": "jest",
    "build:prod": "NODE_ENV='production' webpack --profile --colors --progress",
    "build:publish": "NODE_ENV='production' webpack --config webpack.config.publish.js --profile --colors --progress",
    "serv:dev": "NODE_ENV='development' webpack-dev-server --config webpack.config.dev.js --colors --progress --profile",
    "stats:prod": "NODE_ENV='production' webpack --config webpack.config.stats.js --colors --progress --profile"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "bundle-loader": "^0.5.5",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.4",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "react-test-renderer": "^16.0.0",
    "style-loader": "^0.19.0",
    "stylelint": "8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "url-loader": "^0.6.2",
    "webpack": "3.8.0",
    "webpack-bundle-analyzer": "^2.3.0",
    "webpack-dev-server": "^2.9.2",
    "webpack-merge": "^4.1.0",
    "wepack": "^0.0.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bootstrap": "^4.0.0-beta",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "modern-toastr": "^1.1.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "store": "^2.0.12",
    "styled-components": "^2.2.1"
  }
}
